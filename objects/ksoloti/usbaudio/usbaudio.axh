      <params/>
      <attribs/>
   </obj>
   <comment type="patch/comment" x="126" y="196" text="Two sine wave test signals from Ksoloti..."/>
   <comment type="patch/comment" x="350" y="196" text="...are being sent to the DAW&apos;s (host&apos;s) USB audio inputs."/>
   <obj type="osc/sine" uuid="6e094045cca76a9dbf7ebfa72e44e4700d2b3ba" name="sine_1" x="154" y="224">
      <params>
         <frac32.s.map name="pitch" value="5.0"/>
      </params>
      <attribs/>
   </obj>
   <obj type="ksoloti/usbaudio/usb out stereo" uuid="f39f2910bf8342d18afd9b09bcf110c9" name="usb_2" x="406" y="224">
      <params/>
      <attribs/>
   </obj>
   <obj type="osc/sine" uuid="6e094045cca76a9dbf7ebfa72e44e4700d2b3ba" name="sine_2" x="154" y="336">
      <params>
         <frac32.s.map name="pitch" value="-7.0"/>
      </params>
      <attribs/>
   </obj>
   <nets>
      <net>
         <source obj="usb_1" outlet="left"/>
         <dest obj="out_1" inlet="left"/>
      </net>
      <net>
         <source obj="usb_1" outlet="right"/>
         <dest obj="out_1" inlet="right"/>
      </net>
      <net>
         <source obj="sine_1" outlet="wave"/>
         <dest obj="usb_2" inlet="left"/>
      </net>
      <net>
         <source obj="sine_2" outlet="wave"/>
         <dest obj="usb_2" inlet="right"/>
      </net>
   </nets>
   <settings>
