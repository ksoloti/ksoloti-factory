<patch-1.0 appVersion="1.1.0">
   <comment type="patch/comment" x="14" y="28" text="The signals the host sends out to the Ksoloti USB audio inputs..."/>
   <comment type="patch/comment" x="350" y="28" text="...are being forwarded to Ksoloti&apos;s hardware audio outputs."/>
   <obj type="ksoloti/usbaudio/usb in stereo" uuid="97497df749dd47a19a18f4a78ebc58e6" name="usb_1" x="98" y="56">
      <params/>
      <attribs/>
   </obj>
